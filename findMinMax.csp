	.EQU @, $04F
LARGEST: 	.WORD 0
	
	.EQU @, $04E
SMALLEST: 	.WORD 0

	.EQU @, 000
	LDX# 001
BEGIN:	LDA+ $050
	CMA LARGEST
	JGT STORELARGEST
COMPSMALLEST:	CMA SMALLEST
	JLT STORESMALLEST	
TOINDEXCHECK:	ADX# 001
	CMX $050
	JLE BEGIN
	JGT END ;CHECK EQUAL TO CASE AND WHAT TO DO ABT THAT

END:	HLT

	.EQU @,020
STORELARGEST:	STA LARGEST; MAKE SURE NO OVERWRITES
	JMP COMPSMALLEST
	
	.EQU @,030
STORESMALLEST:	STA SMALLEST
	JMP TOINDEXCHECK